with open('task5', 'r', encoding='utf-8') as file:
    lines = file.readlines()

if len(lines) < 3:
    raise ValueError("Недостаточно данных")

a = int(lines[0].strip())
b = int(lines[1].strip())
c = int(lines[2].strip())

result = a / b + c

with open('output', 'w', encoding='utf-8') as file:
    file.write(str(result))
